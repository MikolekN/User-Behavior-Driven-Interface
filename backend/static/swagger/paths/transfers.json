{
    "get": {
        "summary": "Get Transfers connected with current user",
        "responses": {
            "200": {
                "description": "Successful transfers response",
                "content": {
                    "application.json": {
                        "schema": {
                            "type": "object",
                            "properties": {
                                "message": {
                                    "type": "string"
                                },
                                "transfers": {
                                    "properties": {
                                        "_id": {
                                            "type": "string"
                                        },
                                        "amount": {
                                            "type": "number",
                                            "format": "float"
                                        },
                                        "created": {
                                            "type": "string"
                                        },
                                        "income": {
                                            "type": "boolean"
                                        },
                                        "issuer_name": {
                                            "type": "string"
                                        },
                                        "title": {
                                            "type": "string"
                                        },
                                        "transfer_from_id": {
                                            "type": "string"
                                        },
                                        "transfer_to_id": {
                                            "type": "string"
                                        }
                                    }
                                }
                            }
                        }
                    }
                }
            },
            "401": {
                "description": "No user logged in",
                "content": {
                    "application/json": {
                        "schema": {
                            "type": "object",
                            "properties": {
                                "message": {
                                    "type": "string"
                                }
                            }
                        }
                    }
                }
            }
        }
    }
}