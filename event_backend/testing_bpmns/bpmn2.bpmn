<?xml version="1.0" encoding="UTF-8"?>
<bpmn:definitions xmlns:bpmn="http://www.omg.org/spec/BPMN/20100524/MODEL"
                  xmlns:bpmndi="http://www.omg.org/spec/BPMN/20100524/DI"
                  xmlns:omgdc="http://www.omg.org/spec/DD/20100524/DC"
                  xmlns:omgdi="http://www.omg.org/spec/DD/20100524/DI"
                  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                  targetNamespace="http://example.com/bpmn">
  <bpmn:process id="split-process" isExecutable="false">

    <!-- Start Event -->
    <bpmn:startEvent id="start" name="start">
      <bpmn:outgoing>flow_start_to_split</bpmn:outgoing>
    </bpmn:startEvent>

    <!-- Exclusive Split Gateway -->
    <bpmn:exclusiveGateway id="split_gateway" name="Split">
      <bpmn:incoming>flow_start_to_split</bpmn:incoming>
      <bpmn:outgoing>flow_split_to_transactions</bpmn:outgoing>
      <bpmn:outgoing>flow_split_to_cyclic</bpmn:outgoing>
    </bpmn:exclusiveGateway>

    <!-- First Task -->
    <bpmn:task id="task_transactions" name="page_transition_event /transactions">
      <bpmn:incoming>flow_split_to_transactions</bpmn:incoming>
      <bpmn:outgoing>flow_transactions_to_merge</bpmn:outgoing>
    </bpmn:task>

    <!-- Second Task -->
    <bpmn:task id="task_cyclic" name="page_transition_event /cyclic-payments">
      <bpmn:incoming>flow_split_to_cyclic</bpmn:incoming>
      <bpmn:outgoing>flow_cyclic_to_merge</bpmn:outgoing>
    </bpmn:task>

    <!-- Exclusive Join Gateway -->
    <bpmn:exclusiveGateway id="merge_gateway" name="Merge">
      <bpmn:incoming>flow_transactions_to_merge</bpmn:incoming>
      <bpmn:incoming>flow_cyclic_to_merge</bpmn:incoming>
      <bpmn:outgoing>flow_merge_to_end</bpmn:outgoing>
    </bpmn:exclusiveGateway>

    <!-- End Event -->
    <bpmn:endEvent id="end" name="end">
      <bpmn:incoming>flow_merge_to_end</bpmn:incoming>
    </bpmn:endEvent>

    <!-- Sequence Flows -->
    <bpmn:sequenceFlow id="flow_start_to_split" sourceRef="start" targetRef="split_gateway"/>
    <bpmn:sequenceFlow id="flow_split_to_transactions" sourceRef="split_gateway" targetRef="task_transactions"/>
    <bpmn:sequenceFlow id="flow_split_to_cyclic" sourceRef="split_gateway" targetRef="task_cyclic"/>
    <bpmn:sequenceFlow id="flow_transactions_to_merge" sourceRef="task_transactions" targetRef="merge_gateway"/>
    <bpmn:sequenceFlow id="flow_cyclic_to_merge" sourceRef="task_cyclic" targetRef="merge_gateway"/>
    <bpmn:sequenceFlow id="flow_merge_to_end" sourceRef="merge_gateway" targetRef="end"/>

  </bpmn:process>

  <!-- Diagram Layout -->
  <bpmndi:BPMNDiagram id="BPMNDiagram_split">
    <bpmndi:BPMNPlane id="BPMNPlane_split" bpmnElement="split-process">

      <!-- Start Event -->
      <bpmndi:BPMNShape id="shape_start" bpmnElement="start">
        <omgdc:Bounds x="100" y="150" width="36" height="36"/>
      </bpmndi:BPMNShape>

      <!-- Split Gateway -->
      <bpmndi:BPMNShape id="shape_split_gateway" bpmnElement="split_gateway" isMarkerVisible="true">
        <omgdc:Bounds x="180" y="145" width="50" height="50"/>
      </bpmndi:BPMNShape>

      <!-- Task Transactions -->
      <bpmndi:BPMNShape id="shape_task_transactions" bpmnElement="task_transactions">
        <omgdc:Bounds x="300" y="80" width="180" height="50"/>
      </bpmndi:BPMNShape>

      <!-- Task Cyclic -->
      <bpmndi:BPMNShape id="shape_task_cyclic" bpmnElement="task_cyclic">
        <omgdc:Bounds x="300" y="210" width="180" height="50"/>
      </bpmndi:BPMNShape>

      <!-- Merge Gateway -->
      <bpmndi:BPMNShape id="shape_merge_gateway" bpmnElement="merge_gateway" isMarkerVisible="true">
        <omgdc:Bounds x="520" y="145" width="50" height="50"/>
      </bpmndi:BPMNShape>

      <!-- End Event -->
      <bpmndi:BPMNShape id="shape_end" bpmnElement="end">
        <omgdc:Bounds x="620" y="150" width="36" height="36"/>
      </bpmndi:BPMNShape>

      <!-- Sequence Flows -->
      <bpmndi:BPMNEdge id="edge_flow_start_to_split" bpmnElement="flow_start_to_split">
        <omgdi:waypoint x="136" y="168"/>
        <omgdi:waypoint x="180" y="170"/>
      </bpmndi:BPMNEdge>

      <bpmndi:BPMNEdge id="edge_flow_split_to_transactions" bpmnElement="flow_split_to_transactions">
        <omgdi:waypoint x="205" y="145"/>
        <omgdi:waypoint x="205" y="105"/>
        <omgdi:waypoint x="300" y="105"/>
      </bpmndi:BPMNEdge>

      <bpmndi:BPMNEdge id="edge_flow_split_to_cyclic" bpmnElement="flow_split_to_cyclic">
        <omgdi:waypoint x="205" y="195"/>
        <omgdi:waypoint x="205" y="235"/>
        <omgdi:waypoint x="300" y="235"/>
      </bpmndi:BPMNEdge>

      <bpmndi:BPMNEdge id="edge_flow_transactions_to_merge" bpmnElement="flow_transactions_to_merge">
        <omgdi:waypoint x="480" y="105"/>
        <omgdi:waypoint x="545" y="105"/>
        <omgdi:waypoint x="545" y="145"/>
      </bpmndi:BPMNEdge>

      <bpmndi:BPMNEdge id="edge_flow_cyclic_to_merge" bpmnElement="flow_cyclic_to_merge">
        <omgdi:waypoint x="480" y="235"/>
        <omgdi:waypoint x="545" y="235"/>
        <omgdi:waypoint x="545" y="195"/>
      </bpmndi:BPMNEdge>

      <bpmndi:BPMNEdge id="edge_flow_merge_to_end" bpmnElement="flow_merge_to_end">
        <omgdi:waypoint x="570" y="170"/>
        <omgdi:waypoint x="620" y="168"/>
      </bpmndi:BPMNEdge>

    </bpmndi:BPMNPlane>
  </bpmndi:BPMNDiagram>
</bpmn:definitions>
